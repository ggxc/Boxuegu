<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.edu.gdmec.android.boxuegu">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="博学谷"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.RegisterActivity" />

        <activity android:name=".activity.SettingActivity" />
        <activity android:name=".activity.FindPwdActivity" />
        <activity android:name=".activity.ModifyPwdActivity"></activity>
        <activity android:name=".activity.ActivityUserInfoActivity"/>
        <activity android:name=".activity.ActivityChangeUserInfoActivity"/>
        <activity android:name=".activity.ActivityExercisesDetailActivity"/>
        <activity android:name=".activity.VideoListActivity"/>
        <activity android:name=".activity.VideoPlayActivity"/>
        <activity android:name=".activity.PlayHistoryActivity"/>
    </application>

</manifest>